<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd">
    <!--
        Added the constraints for entity Plan.
    -->
    <changeSet id="20191030213641-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="owner_id"
                                 baseTableName="plan"
                                 constraintName="fk_plan_owner_id"
                                 referencedColumnNames="id"
                                 referencedTableName="jhi_user"/>

        <addForeignKeyConstraint baseColumnNames="creator_id"
                                 baseTableName="plan"
                                 constraintName="fk_plan_creator_id"
                                 referencedColumnNames="id"
                                 referencedTableName="jhi_user"/>

        <addForeignKeyConstraint baseColumnNames="excercise_config_id"
                                 baseTableName="plan"
                                 constraintName="fk_plan_excercise_config_id"
                                 referencedColumnNames="id"
                                 referencedTableName="excercise_config"/>

    </changeSet>
</databaseChangeLog>
